#include <stdio.h>
#include <string.h>

int main() {
    char names[4][21];
    int i;

    // Input names
    for (i = 0; i < 4; i++) {
        printf("Enter name %d: ", i+1);
        scanf("%s", names[i]);
    }

    // Print names in reverse order
    for (i = 3; i >= 0; i--) {
        printf("%s\n", names[i]);
    }

    return 0;
}
